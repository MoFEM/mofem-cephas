h2 = 0.414;

str1='brick x ' + str(Lx) + ' y ' + str(Ly) +' z ' + str(h1);   cubit.cmd(str1)
str1='brick x ' + str(Lx) + ' y ' + str(Ly) +' z ' + str(h2);   cubit.cmd(str1)
str1='brick x ' + str(Lx) + ' y ' + str(Ly) +' z ' + str(h2);   cubit.cmd(str1)

