******************************************************************************************
*                                                                                        *
* Example run MsFE_Degradation_Reliability_Laminate_Disp                                 *
*  for performing time-dependent reliability analysis with consideration temperature and *
*  moisture induced degradation                                                          *
*                                                                                        *
******************************************************************************************

# Step 1: thermal conductivity

make ../degradation_model/Thermal_Disp_MRhs && \
mpirun -np 1 ../degradation_model/Thermal_Disp_MRhs \
-my_file /mnt/home/MyBuild/mofem-cephas/mofem_v0.2/users_modules/degradation_model/meshes/Using_Textile_RVE/Textile_RVE_Thermal.cub \
-ksp_type fgmres -pc_type lu -pc_factor_mat_solver_package superlu_dist \
-ksp_monitor -my_output_file_Dmat thermal_Dmat.out -my_order 1

# Step 2: moisture diffusivity

make ../degradation_model/MoistureTrans_Disp_MRhs_with_incl && \
mpirun -np 1 ../degradation_model/MoistureTrans_Disp_MRhs_with_incl \
-my_file /mnt/home/MyBuild/mofem-cephas/mofem_v0.2/users_modules/degradation_model/meshes/Using_Textile_RVE/Textile_RVE_moisture.cub \
-ksp_type fgmres -pc_type lu -pc_factor_mat_solver_package superlu_dist \
-ksp_monitor -my_output_file_Dmat diffusion_Dmat.out -my_order 1

# Step 3: Unsteady thermal macro problem 

make ../degradation_model/thermal_unsteady_inputDmat && \
mpirun -np 1 ../degradation_model/thermal_unsteady_inputDmat \
-my_file /mnt/home/MyBuild/mofem-cephas/mofem_v0.2/users_modules/degradation_model/meshes/Using_Textile_RVE/Macro_Plate.cub \
-ksp_type fgmres -pc_type lu -pc_factor_mat_solver_package superlu_dist -ksp_monitor \
-snes_type newtonls -snes_linesearch_type basic -snes_max_it 100 -snes_atol 1e-8 -snes_rtol 1e-8 -snes_monitor \
-ts_monitor -ts_type beuler -ts_dt 86400 -ts_final_time 864000 -snes_lag_jacobian -2 \
-my_order 1 -my_max_post_proc_ref_level 0
  
# Step 4: Unsteady moisture macro problem   
  
make ../degradation_model/diffusion_unsteady_inputDmat && \
mpirun -np 1 ../degradation_model/diffusion_unsteady_inputDmat \
-my_file solution.h5m \
-ksp_type fgmres -pc_type lu -pc_factor_mat_solver_package superlu_dist -ksp_monitor \
-snes_type newtonls -snes_linesearch_type basic -snes_max_it 100 -snes_atol 1e-8 -snes_rtol 1e-8 -snes_monitor \
-ts_monitor -ts_type beuler -ts_dt 86400 -ts_final_time 864000 -snes_lag_jacobian -2 \
-my_order 1 -my_max_post_proc_ref_level 0  

# Step 5: Calculate wt which will be used to calculate degredaiton (1-wt) (time here in days)

make ../degradation_model/wt_unsteady_macro && \
mpirun -np 1 ../degradation_model/wt_unsteady_macro \
-my_file solution_mois.h5m \
-ksp_type fgmres -pc_type lu -pc_factor_mat_solver_package superlu_dist -ksp_monitor \
-snes_type newtonls -snes_linesearch_type basic -snes_max_it 100 -snes_atol 1e-8 -snes_rtol 1e-8 -snes_monitor \
-ts_monitor -ts_type beuler -ts_dt 1 -ts_final_time 10 -snes_lag_jacobian -2 \
-my_order 1 -my_max_post_proc_ref_level 0 \
-my_saturation_conc 1

# Step 6: Run potential flow calculation to identify fibre direction

mpirun -np 1 ../homogenisation/TransIso_Potential_Flow_NoInt \
-my_file /mnt/home/MyBuild/mofem-cephas/mofem_v0.2/users_modules/degradation_model/meshes/trans_iso/Textile_RVE_Mechanical.cub \
