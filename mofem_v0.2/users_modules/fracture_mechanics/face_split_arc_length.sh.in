#!/bin/sh

while test $# -gt 0; do
        case "$1" in
                -h|--help)
			echo "crack propagation with face splitting"
			echo "sets: -a 0 -s 1"
			./arc_length.sh --help
                        exit 0
                        ;;
		*)
                        break
                        ;;
        esac
done



./arc_length.sh $@ -a 0 -s 1
