# Set KSP solver
-ksp_type fgmres
-pc_type lu
-pc_factor_mat_solver_packag mumps
-ksp_atol 1e-12
-ksp_rtol 0
-ksp_monitor ascii

-pc_fieldsplit_type schur
-pc_fieldsplit_schur_precondition self
-fieldsplit_0_ksp_type minres
-fieldsplit_0_ksp_max_it 500
-fieldsplit_0_ksp_rtol 1e-4
-fieldsplit_0_pc_type hypre
-fieldsplit_0_pc_hypre_type parasails
-fieldsplit_0_pc_hypre_parasails_sym SPD
-fieldsplit_1_ksp_type preonly
-fieldsplit_1_pc_type lsc
-fieldsplit_1_lsc_ksp_type minres
-fieldsplit_1_lsc_ksp_rtol 1e-8
-fieldsplit_1_lsc_ksp_max_it 500
-fieldsplit_1_lsc_pc_type hypre
-fieldsplit_1_lsc_pc_hypre_type parasails
-fieldsplit_1_lsc_pc_hypre_parasails_sym SPD
-fieldsplit_1_lsc_pc_hypre_parasails_nlevels 2
-fieldsplit_1_lsc_pc_hypre_parasails_thresh 0.01
-fieldsplit_1_lsc_pc_hypre_parasails_logging 1
