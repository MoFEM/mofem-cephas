#Generate fibre direction in Model using Potential Flow Problem

make && mpirun -np 1 ./potential_flow_disp -my_file ~/Desktop/RVE_Cubit/PBC/RVE_Disp.cub -ksp_type fgmres -pc_type lu -pc_factor_mat_solver_package superlu_dist -ksp_monitor -my_order 1


#example how to run the actual problem

make && mpirun -np 1 ./interface_fibre_new_ver_RVE_Thesis -my_file solution.h5m -ksp_type fgmres -pc_type asm -sub_pc_type lu -ksp_monitor -my_order 1

#Output

out.vtk - computational mesh
out_post_proc.vtk - refinded mesh of TETs for prostrocesing
out_post_proc_prisms.vtk - refinded mesh of PRISMs for prostrocesing
