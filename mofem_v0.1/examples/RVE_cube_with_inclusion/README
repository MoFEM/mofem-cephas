make && mpirun -np 1 ./moisture_RVE_Cube_with_inclusion_Periodic -my_file ~/Documents/Post-Doc\ Data/Analysis/Meshes/moisture/Textile_RVE.cub -ksp_type fgmres -pc_type lu -pc_factor_mat_solver_package superlu_dist -ksp_monitor -my_order 1 -myapplied_strain 1.0,0.0,0.0



